#regla para evitar uso de .php 
RewriteEngine on
RewriteCond %{REQUEST_FILENAME} !-d
RewriteCond %{REQUEST_FILENAME}\.php -f
RewriteRule ^(,*)$ $1.php



RewriteRule ^login$ Php/Login.php [L]
RewriteRule ^inicio$ Php/inicio.php [L]
RewriteRule ^encuesta$ Php/encuesta.php [L]
RewriteRule ^login_usuario_be$ Php/login_usuario_be.php [L]
RewriteRule ^addAsesores$ Php/AddAsesores.php [L]
RewriteRule ^cerrar$ Php/CerrarSesion.php [L]
RewriteRule ^agregar$ Php/AddAlumnos.php [L]
RewriteRule ^VerAsesores$ Php/VerAsesores.php [L]
RewriteRule ^AgregarAsesores$ Php/AgregarA.php [L]
